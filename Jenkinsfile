pipeline {
    agent any

    stages {
        stage ('Compile Stage') {

            steps {
                    sh 'echo compile'
                }
            }
        }

        stage ('Testing Stage') {

            steps {
                    sh 'echo test'
                }
            }
        }


        stage ('Deployment Stage') {
            steps {
                    sh 'echo deploy'
                }
            }
        }
    }
}
